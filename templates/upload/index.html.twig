{% extends 'base.html.twig' %}

{% block title %}Tissue!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="container">
	

   {{ form_start(form) }}
   <div style="margin-top: 50px">
   	<div  class="card">
   		<div class="card-body">
   			<h5 class="card-header">Upload des images du tissue</h5>
   			<div class="form-group">
	         
	    
	   </div>    
	   <div class="form-group">
	       {{ form_widget(form.name,{'attr':{'class':'form-control'}}) }}   	
	    </div> 
	    <div class="form-group">{{ form_widget(form.Upload,{'attr':{'class':'btn btn-primary'}}) }}</div>

   		</div>
 
	</div>  
   	
   </div>
   	    

   {{ form_end(form) }}
 
</div>

{% endblock %}

